################################################
#
# timespec64_sub
#
################################################
PK_KTIME_TIMESPEC64_SUB_NAME        := timespec64_sub
PK_KTIME_TIMESPEC64_SUB_VERSION     := $(CONFIG_PK_KTIME_TIMESPEC64_SUB_VERSION)
PK_KTIME_TIMESPEC64_SUB_SITE        := $(CONFIG_PK_KTIME_TIMESPEC64_SUB_URL)
PK_KTIME_TIMESPEC64_SUB_GITHUB      := $(CONFIG_PK_KTIME_TIMESPEC64_SUB_GITHUB)
PK_KTIME_TIMESPEC64_SUB_SUBNAME     :=
PK_KTIME_TIMESPEC64_SUB_BUILDARCH   := $(CONFIG_PK_KTIME_TIMESPEC64_SUB_BUILDARCH)
PK_KTIME_TIMESPEC64_SUB_DIR         := $(srctree)/package/ktime/timespec64_sub
PK_KTIME_TIMESPEC64_SUB_PATCH       := $(PK_KTIME_TIMESPEC64_SUB_DIR)/patch
PK_KTIME_TIMESPEC64_SUB_BSBIT       := timespec64_sub-$(PK_KTIME_TIMESPEC64_SUB_VERSION).bsbit
PK_KTIME_TIMESPEC64_SUB_OUT         :=
PK_KTIME_TIMESPEC64_SUB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_KTIME_TIMESPEC64_SUB_TAR         := $(CONFIG_PK_KTIME_TIMESPEC64_SUB_TAR)
PK_KTIME_TIMESPEC64_SUB_PACKAGE     :=
PK_KTIME_TIMESPEC64_SUB_CONFIG      := $(CONFIG_PK_KTIME_TIMESPEC64_SUB_CONFIG)
SUB_TARGET               += $(PK_KTIME_TIMESPEC64_SUB_DIR)/build

PK_KTIME_TIMESPEC64_SUB_sh = $(CONFIG_SHELL) $(PK_KTIME_TIMESPEC64_SUB_SCRIPTS) \
                 $1X \
                 $(PK_KTIME_TIMESPEC64_SUB_NAME)X \
                 $(PK_KTIME_TIMESPEC64_SUB_VERSION)X \
                 $(PK_KTIME_TIMESPEC64_SUB_SITE)X \
                 $(PK_KTIME_TIMESPEC64_SUB_GITHUB)X \
                 $(PK_KTIME_TIMESPEC64_SUB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_KTIME_TIMESPEC64_SUB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_KTIME_TIMESPEC64_SUB_SUBNAME)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_CONFIG)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_LIBPATH)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_CPPFLAGS)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_DPKCONFIG)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_CFLAGS)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_LDFLAGS)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_CXXFLAGS)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_ASFLAGS)X \
                 $(PK_KTIME_TIMESPEC64_SUB_BSBIT)X \
                 $(PK_KTIME_TIMESPEC64_SUB_BUILDARCH)X \
		 $(PK_KTIME_TIMESPEC64_SUB_CSRCFILE)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_SRCFILE)X \
                 $(CONFIG_PK_KTIME_TIMESPEC64_SUB_CSRCFILE)

$(PK_KTIME_TIMESPEC64_SUB_DIR)/build: FORCE
	$(Q)$(call PK_KTIME_TIMESPEC64_SUB_sh,$(srctree))
