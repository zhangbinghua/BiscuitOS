################################################
#
# pushsection
#
################################################
PK_ARM_ASM_PUSHSECTION_NAME        := pushsection
PK_ARM_ASM_PUSHSECTION_VERSION     := $(CONFIG_PK_ARM_ASM_PUSHSECTION_VERSION)
PK_ARM_ASM_PUSHSECTION_SITE        := $(CONFIG_PK_ARM_ASM_PUSHSECTION_URL)
PK_ARM_ASM_PUSHSECTION_GITHUB      := $(CONFIG_PK_ARM_ASM_PUSHSECTION_GITHUB)
PK_ARM_ASM_PUSHSECTION_SUBNAME     :=
PK_ARM_ASM_PUSHSECTION_BUILDARCH   := $(CONFIG_PK_ARM_ASM_PUSHSECTION_BUILDARCH)
PK_ARM_ASM_PUSHSECTION_DIR         := $(srctree)/package/arm-asm/pushsection
PK_ARM_ASM_PUSHSECTION_PATCH       := $(PK_ARM_ASM_PUSHSECTION_DIR)/patch
PK_ARM_ASM_PUSHSECTION_BSBIT       := pushsection-$(PK_ARM_ASM_PUSHSECTION_VERSION).bsbit
PK_ARM_ASM_PUSHSECTION_OUT         :=
PK_ARM_ASM_PUSHSECTION_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_ARM_ASM_PUSHSECTION_TAR         := $(CONFIG_PK_ARM_ASM_PUSHSECTION_TAR)
PK_ARM_ASM_PUSHSECTION_PACKAGE     :=
PK_ARM_ASM_PUSHSECTION_CONFIG      := $(CONFIG_PK_ARM_ASM_PUSHSECTION_CONFIG)
SUB_TARGET                  += $(PK_ARM_ASM_PUSHSECTION_DIR)/build

PK_ARM_ASM_PUSHSECTION_sh = $(CONFIG_SHELL) $(PK_ARM_ASM_PUSHSECTION_SCRIPTS) \
                 $1X \
                 $(PK_ARM_ASM_PUSHSECTION_NAME)X \
                 $(PK_ARM_ASM_PUSHSECTION_VERSION)X \
                 $(PK_ARM_ASM_PUSHSECTION_SITE)X \
                 $(PK_ARM_ASM_PUSHSECTION_GITHUB)X \
                 $(PK_ARM_ASM_PUSHSECTION_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_ARM_ASM_PUSHSECTION_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_ARM_ASM_PUSHSECTION_SUBNAME)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_CONFIG)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_LIBPATH)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_CPPFLAGS)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_DPKCONFIG)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_CFLAGS)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_LDFLAGS)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_CXXFLAGS)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_ASFLAGS)X \
                 $(PK_ARM_ASM_PUSHSECTION_BSBIT)X \
                 $(PK_ARM_ASM_PUSHSECTION_BUILDARCH)X \
		 $(PK_ARM_ASM_PUSHSECTION_CSRCFILE)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_SRCFILE)X \
                 $(CONFIG_PK_ARM_ASM_PUSHSECTION_CSRCFILE)

$(PK_ARM_ASM_PUSHSECTION_DIR)/build: FORCE
	$(Q)$(call PK_ARM_ASM_PUSHSECTION_sh,$(srctree))
