################################################
#
# DEFINE_PER_CPU_SHARED_ALIGNED
#
################################################
PK_DEFINE_PER_CPU_SHARED_ALIGNED_NAME        := DEFINE_PER_CPU_SHARED_ALIGNED
PK_DEFINE_PER_CPU_SHARED_ALIGNED_VERSION     := $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_VERSION)
PK_DEFINE_PER_CPU_SHARED_ALIGNED_SITE        := $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_URL)
PK_DEFINE_PER_CPU_SHARED_ALIGNED_GITHUB      := $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_GITHUB)
PK_DEFINE_PER_CPU_SHARED_ALIGNED_SUBNAME     :=
PK_DEFINE_PER_CPU_SHARED_ALIGNED_BUILDARCH   := $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_BUILDARCH)
PK_DEFINE_PER_CPU_SHARED_ALIGNED_DIR         := $(srctree)/package/percpu/DEFINE_PER_CPU_SHARED_ALIGNED
PK_DEFINE_PER_CPU_SHARED_ALIGNED_PATCH       := $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_DIR)/patch
PK_DEFINE_PER_CPU_SHARED_ALIGNED_BSBIT       := DEFINE_PER_CPU_SHARED_ALIGNED-$(PK_DEFINE_PER_CPU_SHARED_ALIGNED_VERSION).bsbit
PK_DEFINE_PER_CPU_SHARED_ALIGNED_OUT         :=
PK_DEFINE_PER_CPU_SHARED_ALIGNED_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_DEFINE_PER_CPU_SHARED_ALIGNED_TAR         := $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_TAR)
PK_DEFINE_PER_CPU_SHARED_ALIGNED_PACKAGE     :=
PK_DEFINE_PER_CPU_SHARED_ALIGNED_CONFIG      := $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_CONFIG)
SUB_TARGET                  += $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_DIR)/build

PK_DEFINE_PER_CPU_SHARED_ALIGNED_sh = $(CONFIG_SHELL) $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_SCRIPTS) \
                 $1X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_NAME)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_VERSION)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_SITE)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_GITHUB)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_SUBNAME)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_CONFIG)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_LIBPATH)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_CPPFLAGS)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_DPKCONFIG)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_CFLAGS)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_LDFLAGS)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_CXXFLAGS)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_ASFLAGS)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_BSBIT)X \
                 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_BUILDARCH)X \
		 $(PK_DEFINE_PER_CPU_SHARED_ALIGNED_CSRCFILE)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_SRCFILE)X \
                 $(CONFIG_PK_DEFINE_PER_CPU_SHARED_ALIGNED_CSRCFILE)

$(PK_DEFINE_PER_CPU_SHARED_ALIGNED_DIR)/build: FORCE
	$(Q)$(call PK_DEFINE_PER_CPU_SHARED_ALIGNED_sh,$(srctree))
