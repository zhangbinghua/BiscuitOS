################################################
#
# percpu_counter_inc
#
################################################
PK_PERCPU_COUNTER_INC_NAME        := percpu_counter_inc
PK_PERCPU_COUNTER_INC_VERSION     := $(CONFIG_PK_PERCPU_COUNTER_INC_VERSION)
PK_PERCPU_COUNTER_INC_SITE        := $(CONFIG_PK_PERCPU_COUNTER_INC_URL)
PK_PERCPU_COUNTER_INC_GITHUB      := $(CONFIG_PK_PERCPU_COUNTER_INC_GITHUB)
PK_PERCPU_COUNTER_INC_SUBNAME     :=
PK_PERCPU_COUNTER_INC_BUILDARCH   := $(CONFIG_PK_PERCPU_COUNTER_INC_BUILDARCH)
PK_PERCPU_COUNTER_INC_DIR         := $(srctree)/package/percpu/percpu_counter_inc
PK_PERCPU_COUNTER_INC_PATCH       := $(PK_PERCPU_COUNTER_INC_DIR)/patch
PK_PERCPU_COUNTER_INC_BSBIT       := percpu_counter_inc-$(PK_PERCPU_COUNTER_INC_VERSION).bsbit
PK_PERCPU_COUNTER_INC_OUT         :=
PK_PERCPU_COUNTER_INC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PERCPU_COUNTER_INC_TAR         := $(CONFIG_PK_PERCPU_COUNTER_INC_TAR)
PK_PERCPU_COUNTER_INC_PACKAGE     :=
PK_PERCPU_COUNTER_INC_CONFIG      := $(CONFIG_PK_PERCPU_COUNTER_INC_CONFIG)
SUB_TARGET                  += $(PK_PERCPU_COUNTER_INC_DIR)/build

PK_PERCPU_COUNTER_INC_sh = $(CONFIG_SHELL) $(PK_PERCPU_COUNTER_INC_SCRIPTS) \
                 $1X \
                 $(PK_PERCPU_COUNTER_INC_NAME)X \
                 $(PK_PERCPU_COUNTER_INC_VERSION)X \
                 $(PK_PERCPU_COUNTER_INC_SITE)X \
                 $(PK_PERCPU_COUNTER_INC_GITHUB)X \
                 $(PK_PERCPU_COUNTER_INC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PERCPU_COUNTER_INC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PERCPU_COUNTER_INC_SUBNAME)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_CONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_LIBPATH)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_CPPFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_DPKCONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_CFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_LDFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_CXXFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_ASFLAGS)X \
                 $(PK_PERCPU_COUNTER_INC_BSBIT)X \
                 $(PK_PERCPU_COUNTER_INC_BUILDARCH)X \
		 $(PK_PERCPU_COUNTER_INC_CSRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_SRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_INC_CSRCFILE)

$(PK_PERCPU_COUNTER_INC_DIR)/build: FORCE
	$(Q)$(call PK_PERCPU_COUNTER_INC_sh,$(srctree))
