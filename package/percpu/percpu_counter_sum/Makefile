################################################
#
# percpu_counter_sum
#
################################################
PK_PERCPU_COUNTER_SUM_NAME        := percpu_counter_sum
PK_PERCPU_COUNTER_SUM_VERSION     := $(CONFIG_PK_PERCPU_COUNTER_SUM_VERSION)
PK_PERCPU_COUNTER_SUM_SITE        := $(CONFIG_PK_PERCPU_COUNTER_SUM_URL)
PK_PERCPU_COUNTER_SUM_GITHUB      := $(CONFIG_PK_PERCPU_COUNTER_SUM_GITHUB)
PK_PERCPU_COUNTER_SUM_SUBNAME     :=
PK_PERCPU_COUNTER_SUM_BUILDARCH   := $(CONFIG_PK_PERCPU_COUNTER_SUM_BUILDARCH)
PK_PERCPU_COUNTER_SUM_DIR         := $(srctree)/package/percpu/percpu_counter_sum
PK_PERCPU_COUNTER_SUM_PATCH       := $(PK_PERCPU_COUNTER_SUM_DIR)/patch
PK_PERCPU_COUNTER_SUM_BSBIT       := percpu_counter_sum-$(PK_PERCPU_COUNTER_SUM_VERSION).bsbit
PK_PERCPU_COUNTER_SUM_OUT         :=
PK_PERCPU_COUNTER_SUM_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PERCPU_COUNTER_SUM_TAR         := $(CONFIG_PK_PERCPU_COUNTER_SUM_TAR)
PK_PERCPU_COUNTER_SUM_PACKAGE     :=
PK_PERCPU_COUNTER_SUM_CONFIG      := $(CONFIG_PK_PERCPU_COUNTER_SUM_CONFIG)
SUB_TARGET                  += $(PK_PERCPU_COUNTER_SUM_DIR)/build

PK_PERCPU_COUNTER_SUM_sh = $(CONFIG_SHELL) $(PK_PERCPU_COUNTER_SUM_SCRIPTS) \
                 $1X \
                 $(PK_PERCPU_COUNTER_SUM_NAME)X \
                 $(PK_PERCPU_COUNTER_SUM_VERSION)X \
                 $(PK_PERCPU_COUNTER_SUM_SITE)X \
                 $(PK_PERCPU_COUNTER_SUM_GITHUB)X \
                 $(PK_PERCPU_COUNTER_SUM_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PERCPU_COUNTER_SUM_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PERCPU_COUNTER_SUM_SUBNAME)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_CONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_LIBPATH)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_CPPFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_DPKCONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_CFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_LDFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_CXXFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_ASFLAGS)X \
                 $(PK_PERCPU_COUNTER_SUM_BSBIT)X \
                 $(PK_PERCPU_COUNTER_SUM_BUILDARCH)X \
		 $(PK_PERCPU_COUNTER_SUM_CSRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_SRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUM_CSRCFILE)

$(PK_PERCPU_COUNTER_SUM_DIR)/build: FORCE
	$(Q)$(call PK_PERCPU_COUNTER_SUM_sh,$(srctree))
