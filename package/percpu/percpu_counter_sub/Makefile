################################################
#
# percpu_counter_sub
#
################################################
PK_PERCPU_COUNTER_SUB_NAME        := percpu_counter_sub
PK_PERCPU_COUNTER_SUB_VERSION     := $(CONFIG_PK_PERCPU_COUNTER_SUB_VERSION)
PK_PERCPU_COUNTER_SUB_SITE        := $(CONFIG_PK_PERCPU_COUNTER_SUB_URL)
PK_PERCPU_COUNTER_SUB_GITHUB      := $(CONFIG_PK_PERCPU_COUNTER_SUB_GITHUB)
PK_PERCPU_COUNTER_SUB_SUBNAME     :=
PK_PERCPU_COUNTER_SUB_BUILDARCH   := $(CONFIG_PK_PERCPU_COUNTER_SUB_BUILDARCH)
PK_PERCPU_COUNTER_SUB_DIR         := $(srctree)/package/percpu/percpu_counter_sub
PK_PERCPU_COUNTER_SUB_PATCH       := $(PK_PERCPU_COUNTER_SUB_DIR)/patch
PK_PERCPU_COUNTER_SUB_BSBIT       := percpu_counter_sub-$(PK_PERCPU_COUNTER_SUB_VERSION).bsbit
PK_PERCPU_COUNTER_SUB_OUT         :=
PK_PERCPU_COUNTER_SUB_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PERCPU_COUNTER_SUB_TAR         := $(CONFIG_PK_PERCPU_COUNTER_SUB_TAR)
PK_PERCPU_COUNTER_SUB_PACKAGE     :=
PK_PERCPU_COUNTER_SUB_CONFIG      := $(CONFIG_PK_PERCPU_COUNTER_SUB_CONFIG)
SUB_TARGET                  += $(PK_PERCPU_COUNTER_SUB_DIR)/build

PK_PERCPU_COUNTER_SUB_sh = $(CONFIG_SHELL) $(PK_PERCPU_COUNTER_SUB_SCRIPTS) \
                 $1X \
                 $(PK_PERCPU_COUNTER_SUB_NAME)X \
                 $(PK_PERCPU_COUNTER_SUB_VERSION)X \
                 $(PK_PERCPU_COUNTER_SUB_SITE)X \
                 $(PK_PERCPU_COUNTER_SUB_GITHUB)X \
                 $(PK_PERCPU_COUNTER_SUB_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PERCPU_COUNTER_SUB_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PERCPU_COUNTER_SUB_SUBNAME)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_CONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_LIBPATH)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_CPPFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_DPKCONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_CFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_LDFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_CXXFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_ASFLAGS)X \
                 $(PK_PERCPU_COUNTER_SUB_BSBIT)X \
                 $(PK_PERCPU_COUNTER_SUB_BUILDARCH)X \
		 $(PK_PERCPU_COUNTER_SUB_CSRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_SRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_SUB_CSRCFILE)

$(PK_PERCPU_COUNTER_SUB_DIR)/build: FORCE
	$(Q)$(call PK_PERCPU_COUNTER_SUB_sh,$(srctree))
