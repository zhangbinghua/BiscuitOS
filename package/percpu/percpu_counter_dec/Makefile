################################################
#
# percpu_counter_dec
#
################################################
PK_PERCPU_COUNTER_DEC_NAME        := percpu_counter_dec
PK_PERCPU_COUNTER_DEC_VERSION     := $(CONFIG_PK_PERCPU_COUNTER_DEC_VERSION)
PK_PERCPU_COUNTER_DEC_SITE        := $(CONFIG_PK_PERCPU_COUNTER_DEC_URL)
PK_PERCPU_COUNTER_DEC_GITHUB      := $(CONFIG_PK_PERCPU_COUNTER_DEC_GITHUB)
PK_PERCPU_COUNTER_DEC_SUBNAME     :=
PK_PERCPU_COUNTER_DEC_BUILDARCH   := $(CONFIG_PK_PERCPU_COUNTER_DEC_BUILDARCH)
PK_PERCPU_COUNTER_DEC_DIR         := $(srctree)/package/percpu/percpu_counter_dec
PK_PERCPU_COUNTER_DEC_PATCH       := $(PK_PERCPU_COUNTER_DEC_DIR)/patch
PK_PERCPU_COUNTER_DEC_BSBIT       := percpu_counter_dec-$(PK_PERCPU_COUNTER_DEC_VERSION).bsbit
PK_PERCPU_COUNTER_DEC_OUT         :=
PK_PERCPU_COUNTER_DEC_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_PERCPU_COUNTER_DEC_TAR         := $(CONFIG_PK_PERCPU_COUNTER_DEC_TAR)
PK_PERCPU_COUNTER_DEC_PACKAGE     :=
PK_PERCPU_COUNTER_DEC_CONFIG      := $(CONFIG_PK_PERCPU_COUNTER_DEC_CONFIG)
SUB_TARGET                  += $(PK_PERCPU_COUNTER_DEC_DIR)/build

PK_PERCPU_COUNTER_DEC_sh = $(CONFIG_SHELL) $(PK_PERCPU_COUNTER_DEC_SCRIPTS) \
                 $1X \
                 $(PK_PERCPU_COUNTER_DEC_NAME)X \
                 $(PK_PERCPU_COUNTER_DEC_VERSION)X \
                 $(PK_PERCPU_COUNTER_DEC_SITE)X \
                 $(PK_PERCPU_COUNTER_DEC_GITHUB)X \
                 $(PK_PERCPU_COUNTER_DEC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PERCPU_COUNTER_DEC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PERCPU_COUNTER_DEC_SUBNAME)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_CONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_LIBPATH)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_CPPFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_DPKCONFIG)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_CFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_LDFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_CXXFLAGS)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_ASFLAGS)X \
                 $(PK_PERCPU_COUNTER_DEC_BSBIT)X \
                 $(PK_PERCPU_COUNTER_DEC_BUILDARCH)X \
		 $(PK_PERCPU_COUNTER_DEC_CSRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_SRCFILE)X \
                 $(CONFIG_PK_PERCPU_COUNTER_DEC_CSRCFILE)

$(PK_PERCPU_COUNTER_DEC_DIR)/build: FORCE
	$(Q)$(call PK_PERCPU_COUNTER_DEC_sh,$(srctree))
