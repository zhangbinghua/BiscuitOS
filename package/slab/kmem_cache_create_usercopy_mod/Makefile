################################################
#
# kmem_cache_create_usercopy_mod
#
################################################
PK_KMEM_CACHE_CREATE_MOD_NAME        := kmem_cache_create_usercopy_mod
PK_KMEM_CACHE_CREATE_MOD_VERSION     := $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_VERSION)
PK_KMEM_CACHE_CREATE_MOD_SITE        := $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_URL)
PK_KMEM_CACHE_CREATE_MOD_GITHUB      := $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_GITHUB)
PK_KMEM_CACHE_CREATE_MOD_SUBNAME     :=
PK_KMEM_CACHE_CREATE_MOD_BUILDARCH   := $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_BUILDARCH)
PK_KMEM_CACHE_CREATE_MOD_DIR         := $(srctree)/package/slab/kmem_cache_create_usercopy_mod
PK_KMEM_CACHE_CREATE_MOD_PATCH       := $(PK_KMEM_CACHE_CREATE_MOD_DIR)/patch
PK_KMEM_CACHE_CREATE_MOD_BSBIT       := KMEM_CACHE_CREATE_MOD-$(PK_KMEM_CACHE_CREATE_MOD_VERSION).bsbit
PK_KMEM_CACHE_CREATE_MOD_OUT         :=
PK_KMEM_CACHE_CREATE_MOD_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_KMEM_CACHE_CREATE_MOD_TAR         := $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_TAR)
PK_KMEM_CACHE_CREATE_MOD_PACKAGE     :=
PK_KMEM_CACHE_CREATE_MOD_CONFIG      := $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_CONFIG)
SUB_TARGET                  += $(PK_KMEM_CACHE_CREATE_MOD_DIR)/build

PK_KMEM_CACHE_CREATE_MOD_sh = $(CONFIG_SHELL) $(PK_KMEM_CACHE_CREATE_MOD_SCRIPTS) \
                 $1X \
                 $(PK_KMEM_CACHE_CREATE_MOD_NAME)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_VERSION)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_SITE)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_GITHUB)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_SUBNAME)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_CONFIG)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_LIBPATH)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_CPPFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_DPKCONFIG)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_CFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_LDFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_CXXFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_ASFLAGS)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_BSBIT)X \
                 $(PK_KMEM_CACHE_CREATE_MOD_BUILDARCH)X \
		 $(PK_KMEM_CACHE_CREATE_MOD_CSRCFILE)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_SRCFILE)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_MOD_CSRCFILE)

$(PK_KMEM_CACHE_CREATE_MOD_DIR)/build: FORCE
	$(Q)$(call PK_KMEM_CACHE_CREATE_MOD_sh,$(srctree))
