################################################
#
# strncpy_from_user_mod
#
################################################
PK_STRNCPY_FROM_USER_MOD_NAME        := strncpy_from_user_mod
PK_STRNCPY_FROM_USER_MOD_VERSION     := $(CONFIG_PK_STRNCPY_FROM_USER_MOD_VERSION)
PK_STRNCPY_FROM_USER_MOD_SITE        := $(CONFIG_PK_STRNCPY_FROM_USER_MOD_URL)
PK_STRNCPY_FROM_USER_MOD_GITHUB      := $(CONFIG_PK_STRNCPY_FROM_USER_MOD_GITHUB)
PK_STRNCPY_FROM_USER_MOD_SUBNAME     :=
PK_STRNCPY_FROM_USER_MOD_BUILDARCH   := $(CONFIG_PK_STRNCPY_FROM_USER_MOD_BUILDARCH)
PK_STRNCPY_FROM_USER_MOD_DIR         := $(srctree)/package/slab/strncpy_from_user_mod
PK_STRNCPY_FROM_USER_MOD_PATCH       := $(PK_STRNCPY_FROM_USER_MOD_DIR)/patch
PK_STRNCPY_FROM_USER_MOD_BSBIT       := STRNCPY_FROM_USER_MOD-$(PK_STRNCPY_FROM_USER_MOD_VERSION).bsbit
PK_STRNCPY_FROM_USER_MOD_OUT         :=
PK_STRNCPY_FROM_USER_MOD_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_STRNCPY_FROM_USER_MOD_TAR         := $(CONFIG_PK_STRNCPY_FROM_USER_MOD_TAR)
PK_STRNCPY_FROM_USER_MOD_PACKAGE     :=
PK_STRNCPY_FROM_USER_MOD_CONFIG      := $(CONFIG_PK_STRNCPY_FROM_USER_MOD_CONFIG)
SUB_TARGET                  += $(PK_STRNCPY_FROM_USER_MOD_DIR)/build

PK_STRNCPY_FROM_USER_MOD_sh = $(CONFIG_SHELL) $(PK_STRNCPY_FROM_USER_MOD_SCRIPTS) \
                 $1X \
                 $(PK_STRNCPY_FROM_USER_MOD_NAME)X \
                 $(PK_STRNCPY_FROM_USER_MOD_VERSION)X \
                 $(PK_STRNCPY_FROM_USER_MOD_SITE)X \
                 $(PK_STRNCPY_FROM_USER_MOD_GITHUB)X \
                 $(PK_STRNCPY_FROM_USER_MOD_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_STRNCPY_FROM_USER_MOD_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_STRNCPY_FROM_USER_MOD_SUBNAME)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_CONFIG)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_LIBPATH)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_CPPFLAGS)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_DPKCONFIG)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_CFLAGS)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_LDFLAGS)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_CXXFLAGS)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_ASFLAGS)X \
                 $(PK_STRNCPY_FROM_USER_MOD_BSBIT)X \
                 $(PK_STRNCPY_FROM_USER_MOD_BUILDARCH)X \
		 $(PK_STRNCPY_FROM_USER_MOD_CSRCFILE)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_SRCFILE)X \
                 $(CONFIG_PK_STRNCPY_FROM_USER_MOD_CSRCFILE)

$(PK_STRNCPY_FROM_USER_MOD_DIR)/build: FORCE
	$(Q)$(call PK_STRNCPY_FROM_USER_MOD_sh,$(srctree))
