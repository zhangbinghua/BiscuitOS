################################################
#
# KMEM_CACHE_USERCOPY
#
################################################
PK_KMEM_CACHE_USERCOPY_NAME        := KMEM_CACHE_USERCOPY
PK_KMEM_CACHE_USERCOPY_VERSION     := $(CONFIG_PK_KMEM_CACHE_USERCOPY_VERSION)
PK_KMEM_CACHE_USERCOPY_SITE        := $(CONFIG_PK_KMEM_CACHE_USERCOPY_URL)
PK_KMEM_CACHE_USERCOPY_GITHUB      := $(CONFIG_PK_KMEM_CACHE_USERCOPY_GITHUB)
PK_KMEM_CACHE_USERCOPY_SUBNAME     :=
PK_KMEM_CACHE_USERCOPY_BUILDARCH   := $(CONFIG_PK_KMEM_CACHE_USERCOPY_BUILDARCH)
PK_KMEM_CACHE_USERCOPY_DIR         := $(srctree)/package/slab/KMEM_CACHE_USERCOPY
PK_KMEM_CACHE_USERCOPY_PATCH       := $(PK_KMEM_CACHE_USERCOPY_DIR)/patch
PK_KMEM_CACHE_USERCOPY_BSBIT       := KMEM_CACHE_USERCOPY-$(PK_KMEM_CACHE_USERCOPY_VERSION).bsbit
PK_KMEM_CACHE_USERCOPY_OUT         :=
PK_KMEM_CACHE_USERCOPY_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_KMEM_CACHE_USERCOPY_TAR         := $(CONFIG_PK_KMEM_CACHE_USERCOPY_TAR)
PK_KMEM_CACHE_USERCOPY_PACKAGE     :=
PK_KMEM_CACHE_USERCOPY_CONFIG      := $(CONFIG_PK_KMEM_CACHE_USERCOPY_CONFIG)
SUB_TARGET                  += $(PK_KMEM_CACHE_USERCOPY_DIR)/build

PK_KMEM_CACHE_USERCOPY_sh = $(CONFIG_SHELL) $(PK_KMEM_CACHE_USERCOPY_SCRIPTS) \
                 $1X \
                 $(PK_KMEM_CACHE_USERCOPY_NAME)X \
                 $(PK_KMEM_CACHE_USERCOPY_VERSION)X \
                 $(PK_KMEM_CACHE_USERCOPY_SITE)X \
                 $(PK_KMEM_CACHE_USERCOPY_GITHUB)X \
                 $(PK_KMEM_CACHE_USERCOPY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_KMEM_CACHE_USERCOPY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_KMEM_CACHE_USERCOPY_SUBNAME)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_CONFIG)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_LIBPATH)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_CPPFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_DPKCONFIG)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_CFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_LDFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_CXXFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_ASFLAGS)X \
                 $(PK_KMEM_CACHE_USERCOPY_BSBIT)X \
                 $(PK_KMEM_CACHE_USERCOPY_BUILDARCH)X \
		 $(PK_KMEM_CACHE_USERCOPY_CSRCFILE)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_SRCFILE)X \
                 $(CONFIG_PK_KMEM_CACHE_USERCOPY_CSRCFILE)

$(PK_KMEM_CACHE_USERCOPY_DIR)/build: FORCE
	$(Q)$(call PK_KMEM_CACHE_USERCOPY_sh,$(srctree))
