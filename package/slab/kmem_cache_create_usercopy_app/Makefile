################################################
#
# kmem_cache_create_usercopy_app
#
################################################
PK_KMEM_CACHE_CREATE_APP_NAME        := kmem_cache_create_usercopy_app
PK_KMEM_CACHE_CREATE_APP_VERSION     := $(CONFIG_PK_KMEM_CACHE_CREATE_APP_VERSION)
PK_KMEM_CACHE_CREATE_APP_SITE        := $(CONFIG_PK_KMEM_CACHE_CREATE_APP_URL)
PK_KMEM_CACHE_CREATE_APP_GITHUB      := $(CONFIG_PK_KMEM_CACHE_CREATE_APP_GITHUB)
PK_KMEM_CACHE_CREATE_APP_SUBNAME     :=
PK_KMEM_CACHE_CREATE_APP_BUILDARCH   := $(CONFIG_PK_KMEM_CACHE_CREATE_APP_BUILDARCH)
PK_KMEM_CACHE_CREATE_APP_DIR         := $(srctree)/package/slab/kmem_cache_create_usercopy_app
PK_KMEM_CACHE_CREATE_APP_PATCH       := $(PK_KMEM_CACHE_CREATE_APP_DIR)/patch
PK_KMEM_CACHE_CREATE_APP_BSBIT       := kmem_cache_create_usercopy_app-$(PK_KMEM_CACHE_CREATE_APP_VERSION).bsbit
PK_KMEM_CACHE_CREATE_APP_OUT         :=
PK_KMEM_CACHE_CREATE_APP_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_KMEM_CACHE_CREATE_APP_TAR         := $(CONFIG_PK_KMEM_CACHE_CREATE_APP_TAR)
PK_KMEM_CACHE_CREATE_APP_PACKAGE     :=
PK_KMEM_CACHE_CREATE_APP_CONFIG      := $(CONFIG_PK_KMEM_CACHE_CREATE_APP_CONFIG)
SUB_TARGET                  += $(PK_KMEM_CACHE_CREATE_APP_DIR)/build

PK_KMEM_CACHE_CREATE_APP_sh = $(CONFIG_SHELL) $(PK_KMEM_CACHE_CREATE_APP_SCRIPTS) \
                 $1X \
                 $(PK_KMEM_CACHE_CREATE_APP_NAME)X \
                 $(PK_KMEM_CACHE_CREATE_APP_VERSION)X \
                 $(PK_KMEM_CACHE_CREATE_APP_SITE)X \
                 $(PK_KMEM_CACHE_CREATE_APP_GITHUB)X \
                 $(PK_KMEM_CACHE_CREATE_APP_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_KMEM_CACHE_CREATE_APP_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_KMEM_CACHE_CREATE_APP_SUBNAME)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_CONFIG)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_LIBPATH)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_CPPFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_DPKCONFIG)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_CFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_LDFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_CXXFLAGS)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_ASFLAGS)X \
                 $(PK_KMEM_CACHE_CREATE_APP_BSBIT)X \
                 $(PK_KMEM_CACHE_CREATE_APP_BUILDARCH)X \
		 $(PK_KMEM_CACHE_CREATE_APP_CSRCFILE)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_SRCFILE)X \
                 $(CONFIG_PK_KMEM_CACHE_CREATE_APP_CSRCFILE)

$(PK_KMEM_CACHE_CREATE_APP_DIR)/build: FORCE
	$(Q)$(call PK_KMEM_CACHE_CREATE_APP_sh,$(srctree))
