################################################
#
# Paging on userspace
#
################################################
PK_PAGING_USERSPACE_NAME        := paging_userspace
PK_PAGING_USERSPACE_VERSION     := $(CONFIG_PK_PAGING_USERSPACE_VERSION)
PK_PAGING_USERSPACE_SITE        := $(CONFIG_PK_PAGING_USERSPACE_URL)
PK_PAGING_USERSPACE_GITHUB      := $(CONFIG_PK_PAGING_USERSPACE_GITHUB)
PK_PAGING_USERSPACE_SUBNAME     :=
PK_PAGING_USERSPACE_BUILDARCH   := $(CONFIG_PK_PAGING_USERSPACE_BUILDARCH)
PK_PAGING_USERSPACE_DIR         := $(srctree)/package/paging/paging_userspace
PK_PAGING_USERSPACE_PATCH       := $(PK_PAGING_USERSPACE_DIR)/patch
PK_PAGING_USERSPACE_BSBIT       := paging_userspace-$(PK_PAGING_USERSPACE_VERSION).bsbit
PK_PAGING_USERSPACE_OUT         :=
PK_PAGING_USERSPACE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PAGING_USERSPACE_TAR         := $(CONFIG_PK_PAGING_USERSPACE_TAR)
PK_PAGING_USERSPACE_PACKAGE     :=
PK_PAGING_USERSPACE_CONFIG      := $(CONFIG_PK_PAGING_USERSPACE_CONFIG)
SUB_TARGET                  += $(PK_PAGING_USERSPACE_DIR)/build

PK_PAGING_USERSPACE_sh = $(CONFIG_SHELL) $(PK_PAGING_USERSPACE_SCRIPTS) \
                 $1X \
                 $(PK_PAGING_USERSPACE_NAME)X \
                 $(PK_PAGING_USERSPACE_VERSION)X \
                 $(PK_PAGING_USERSPACE_SITE)X \
                 $(PK_PAGING_USERSPACE_GITHUB)X \
                 $(PK_PAGING_USERSPACE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PAGING_USERSPACE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PAGING_USERSPACE_SUBNAME)X \
                 $(CONFIG_PK_PAGING_USERSPACE_CONFIG)X \
                 $(CONFIG_PK_PAGING_USERSPACE_LIBPATH)X \
                 $(CONFIG_PK_PAGING_USERSPACE_CPPFLAGS)X \
                 $(CONFIG_PK_PAGING_USERSPACE_DPKCONFIG)X \
                 $(CONFIG_PK_PAGING_USERSPACE_CFLAGS)X \
                 $(CONFIG_PK_PAGING_USERSPACE_LDFLAGS)X \
                 $(CONFIG_PK_PAGING_USERSPACE_CXXFLAGS)X \
                 $(CONFIG_PK_PAGING_USERSPACE_ASFLAGS)X \
                 $(PK_PAGING_USERSPACE_BSBIT)X \
                 $(PK_PAGING_USERSPACE_BUILDARCH)X \
		 $(PK_PAGING_USERSPACE_CSRCFILE)X \
                 $(CONFIG_PK_PAGING_USERSPACE_SRCFILE)X \
                 $(CONFIG_PK_PAGING_USERSPACE_CSRCFILE)

$(PK_PAGING_USERSPACE_DIR)/build: FORCE
	$(Q)$(call PK_PAGING_USERSPACE_sh,$(srctree))
