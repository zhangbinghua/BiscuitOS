################################################
#
# pcf8574_traffic
#
################################################
PK_PCF8574_TRAFFIC_NAME        := pcf8574_traffic
PK_PCF8574_TRAFFIC_VERSION     := $(CONFIG_PK_PCF8574_TRAFFIC_VERSION)
PK_PCF8574_TRAFFIC_SITE        := $(CONFIG_PK_PCF8574_TRAFFIC_URL)
PK_PCF8574_TRAFFIC_GITHUB      := $(CONFIG_PK_PCF8574_TRAFFIC_GITHUB)
PK_PCF8574_TRAFFIC_SUBNAME     :=
PK_PCF8574_TRAFFIC_BUILDARCH   := $(CONFIG_PK_PCF8574_TRAFFIC_BUILDARCH)
PK_PCF8574_TRAFFIC_DIR         := $(srctree)/package/i2c/pcf8574_traffic
PK_PCF8574_TRAFFIC_PATCH       := $(PK_PCF8574_TRAFFIC_DIR)/patch
PK_PCF8574_TRAFFIC_BSBIT       := pcf8574_traffic-$(PK_PCF8574_TRAFFIC_VERSION).bsbit
PK_PCF8574_TRAFFIC_OUT         :=
PK_PCF8574_TRAFFIC_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_PCF8574_TRAFFIC_TAR         := $(CONFIG_PK_PCF8574_TRAFFIC_TAR)
PK_PCF8574_TRAFFIC_PACKAGE     :=
PK_PCF8574_TRAFFIC_CONFIG      := $(CONFIG_PK_PCF8574_TRAFFIC_CONFIG)
SUB_TARGET                  += $(PK_PCF8574_TRAFFIC_DIR)/build

PK_PCF8574_TRAFFIC_sh = $(CONFIG_SHELL) $(PK_PCF8574_TRAFFIC_SCRIPTS) \
                 $1X \
                 $(PK_PCF8574_TRAFFIC_NAME)X \
                 $(PK_PCF8574_TRAFFIC_VERSION)X \
                 $(PK_PCF8574_TRAFFIC_SITE)X \
                 $(PK_PCF8574_TRAFFIC_GITHUB)X \
                 $(PK_PCF8574_TRAFFIC_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_PCF8574_TRAFFIC_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_PCF8574_TRAFFIC_SUBNAME)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_CONFIG)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_LIBPATH)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_CPPFLAGS)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_DPKCONFIG)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_CFLAGS)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_LDFLAGS)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_CXXFLAGS)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_ASFLAGS)X \
                 $(PK_PCF8574_TRAFFIC_BSBIT)X \
                 $(PK_PCF8574_TRAFFIC_BUILDARCH)X \
		 $(PK_PCF8574_TRAFFIC_CSRCFILE)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_SRCFILE)X \
                 $(CONFIG_PK_PCF8574_TRAFFIC_CSRCFILE)

$(PK_PCF8574_TRAFFIC_DIR)/build: FORCE
	$(Q)$(call PK_PCF8574_TRAFFIC_sh,$(srctree))
