################################################
#
# inode_init_owner
#
################################################
PK_FS_INODE_INIT_OWNER_NAME        := inode_init_owner
PK_FS_INODE_INIT_OWNER_VERSION     := $(CONFIG_PK_FS_INODE_INIT_OWNER_VERSION)
PK_FS_INODE_INIT_OWNER_SITE        := $(CONFIG_PK_FS_INODE_INIT_OWNER_URL)
PK_FS_INODE_INIT_OWNER_GITHUB      := $(CONFIG_PK_FS_INODE_INIT_OWNER_GITHUB)
PK_FS_INODE_INIT_OWNER_SUBNAME     :=
PK_FS_INODE_INIT_OWNER_BUILDARCH   := $(CONFIG_PK_FS_INODE_INIT_OWNER_BUILDARCH)
PK_FS_INODE_INIT_OWNER_DIR         := $(srctree)/package/filesystem/vfs/inode_init_owner
PK_FS_INODE_INIT_OWNER_PATCH       := $(PK_FS_INODE_INIT_OWNER_DIR)/patch
PK_FS_INODE_INIT_OWNER_BSBIT       := inode_init_owner-$(PK_FS_INODE_INIT_OWNER_VERSION).bsbit
PK_FS_INODE_INIT_OWNER_OUT         :=
PK_FS_INODE_INIT_OWNER_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_FS_INODE_INIT_OWNER_TAR         := $(CONFIG_PK_FS_INODE_INIT_OWNER_TAR)
PK_FS_INODE_INIT_OWNER_PACKAGE     :=
PK_FS_INODE_INIT_OWNER_CONFIG      := $(CONFIG_PK_FS_INODE_INIT_OWNER_CONFIG)
SUB_TARGET                  += $(PK_FS_INODE_INIT_OWNER_DIR)/build

PK_FS_INODE_INIT_OWNER_sh = $(CONFIG_SHELL) $(PK_FS_INODE_INIT_OWNER_SCRIPTS) \
                 $1X \
                 $(PK_FS_INODE_INIT_OWNER_NAME)X \
                 $(PK_FS_INODE_INIT_OWNER_VERSION)X \
                 $(PK_FS_INODE_INIT_OWNER_SITE)X \
                 $(PK_FS_INODE_INIT_OWNER_GITHUB)X \
                 $(PK_FS_INODE_INIT_OWNER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_FS_INODE_INIT_OWNER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_FS_INODE_INIT_OWNER_SUBNAME)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_CONFIG)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_LIBPATH)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_CPPFLAGS)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_DPKCONFIG)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_CFLAGS)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_LDFLAGS)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_CXXFLAGS)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_ASFLAGS)X \
                 $(PK_FS_INODE_INIT_OWNER_BSBIT)X \
                 $(PK_FS_INODE_INIT_OWNER_BUILDARCH)X \
		 $(PK_FS_INODE_INIT_OWNER_CSRCFILE)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_SRCFILE)X \
                 $(CONFIG_PK_FS_INODE_INIT_OWNER_CSRCFILE)

$(PK_FS_INODE_INIT_OWNER_DIR)/build: FORCE
	$(Q)$(call PK_FS_INODE_INIT_OWNER_sh,$(srctree))
