menuconfig PK_RESOURCEPROTO
	bool "resourceproto"

if PK_RESOURCEPROTO

config PK_RESOURCEPROTO_VERSION
	default "1.2.0"
	string "verstion"

config PK_RESOURCEPROTO_TAR
	default "tar.gz"
	string "tar type"

config PK_RESOURCEPROTO_URL
	default "https://www.x.org/releases/individual/proto/"
	string "url"

config PK_RESOURCEPROTO_GITHUB
	string "GitHub"

config PK_RESOURCEPROTO_SRCFILE
	string "Source code list"

config PK_RESOURCEPROTO_CONFIG
	default "--enable-silent-rules --disable-static"
	string "CONFIG: Project Configure Options"

config PK_RESOURCEPROTO_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_RESOURCEPROTO_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_RESOURCEPROTO_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_RESOURCEPROTO_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_RESOURCEPROTO_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_RESOURCEPROTO_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_RESOURCEPROTO_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_RESOURCEPROTO_BUILDARCH
	default "x86_64-unknown-linux-gnu"
	string "--host options on configure for Architecture"

endif # PK_RESOURCEPROTO
