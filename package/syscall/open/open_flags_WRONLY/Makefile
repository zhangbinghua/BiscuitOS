################################################
#
# open_flags_WRONLY
#
################################################
PK_OPEN_FLAGS_WRONLY_NAME        := open_flags_WRONLY
PK_OPEN_FLAGS_WRONLY_VERSION     := $(CONFIG_PK_OPEN_FLAGS_WRONLY_VERSION)
PK_OPEN_FLAGS_WRONLY_SITE        := $(CONFIG_PK_OPEN_FLAGS_WRONLY_URL)
PK_OPEN_FLAGS_WRONLY_GITHUB      := $(CONFIG_PK_OPEN_FLAGS_WRONLY_GITHUB)
PK_OPEN_FLAGS_WRONLY_SUBNAME     :=
PK_OPEN_FLAGS_WRONLY_BUILDARCH   := $(CONFIG_PK_OPEN_FLAGS_WRONLY_BUILDARCH)
PK_OPEN_FLAGS_WRONLY_DIR         := $(srctree)/package/syscall/open/open_flags_WRONLY
PK_OPEN_FLAGS_WRONLY_PATCH       := $(PK_OPEN_FLAGS_WRONLY_DIR)/patch
PK_OPEN_FLAGS_WRONLY_BSBIT       := open_flags_WRONLY-$(PK_OPEN_FLAGS_WRONLY_VERSION).bsbit
PK_OPEN_FLAGS_WRONLY_OUT         :=
PK_OPEN_FLAGS_WRONLY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_OPEN_FLAGS_WRONLY_TAR         := $(CONFIG_PK_OPEN_FLAGS_WRONLY_TAR)
PK_OPEN_FLAGS_WRONLY_PACKAGE     :=
PK_OPEN_FLAGS_WRONLY_CONFIG      := $(CONFIG_PK_OPEN_FLAGS_WRONLY_CONFIG)
SUB_TARGET                  += $(PK_OPEN_FLAGS_WRONLY_DIR)/build

PK_OPEN_FLAGS_WRONLY_sh = $(CONFIG_SHELL) $(PK_OPEN_FLAGS_WRONLY_SCRIPTS) \
                 $1X \
                 $(PK_OPEN_FLAGS_WRONLY_NAME)X \
                 $(PK_OPEN_FLAGS_WRONLY_VERSION)X \
                 $(PK_OPEN_FLAGS_WRONLY_SITE)X \
                 $(PK_OPEN_FLAGS_WRONLY_GITHUB)X \
                 $(PK_OPEN_FLAGS_WRONLY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_OPEN_FLAGS_WRONLY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_OPEN_FLAGS_WRONLY_SUBNAME)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_CONFIG)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_LIBPATH)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_CPPFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_DPKCONFIG)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_CFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_LDFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_CXXFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_ASFLAGS)X \
                 $(PK_OPEN_FLAGS_WRONLY_BSBIT)X \
                 $(PK_OPEN_FLAGS_WRONLY_BUILDARCH)X \
		 $(PK_OPEN_FLAGS_WRONLY_CSRCFILE)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_SRCFILE)X \
                 $(CONFIG_PK_OPEN_FLAGS_WRONLY_CSRCFILE)

$(PK_OPEN_FLAGS_WRONLY_DIR)/build: FORCE
	$(Q)$(call PK_OPEN_FLAGS_WRONLY_sh,$(srctree))
