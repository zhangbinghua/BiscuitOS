menuconfig PK_OPEN_PATHLEN
	select PK_OPEN_KAPI
	bool "open: path length"

if PK_OPEN_PATHLEN

config PK_OPEN_PATHLEN_VERSION
	default "0.0.1"
	string "verstion"

config PK_OPEN_PATHLEN_TAR
	default "tar.gz"
	string "tar type"

config PK_OPEN_PATHLEN_URL
	default "https://gitee.com/BiscuitOS_team/HardStack/raw/Gitee/SystemCall/arm/open/pathlen"
	string "url"

config PK_OPEN_PATHLEN_GITHUB
	string "GitHub"

config PK_OPEN_PATHLEN_SRCFILE
	default "open.c Makefile"
	string "Source code list"

config PK_OPEN_PATHLEN_CSRCFILE
	string "Compile source code list"

config PK_OPEN_PATHLEN_CONFIG
	string "CONFIG: Project Configure Options"

config PK_OPEN_PATHLEN_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_OPEN_PATHLEN_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_OPEN_PATHLEN_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_OPEN_PATHLEN_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_OPEN_PATHLEN_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_OPEN_PATHLEN_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_OPEN_PATHLEN_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_OPEN_PATHLEN_BUILDARCH
	string "--host options on configure for Architecture"

endif # PK_OPEN_PATHLEN
