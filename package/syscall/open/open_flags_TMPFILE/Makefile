################################################
#
# open_flags_TMPFILE
#
################################################
PK_OPEN_FLAGS_TMPFILE_NAME        := open_flags_TMPFILE
PK_OPEN_FLAGS_TMPFILE_VERSION     := $(CONFIG_PK_OPEN_FLAGS_TMPFILE_VERSION)
PK_OPEN_FLAGS_TMPFILE_SITE        := $(CONFIG_PK_OPEN_FLAGS_TMPFILE_URL)
PK_OPEN_FLAGS_TMPFILE_GITHUB      := $(CONFIG_PK_OPEN_FLAGS_TMPFILE_GITHUB)
PK_OPEN_FLAGS_TMPFILE_SUBNAME     :=
PK_OPEN_FLAGS_TMPFILE_BUILDARCH   := $(CONFIG_PK_OPEN_FLAGS_TMPFILE_BUILDARCH)
PK_OPEN_FLAGS_TMPFILE_DIR         := $(srctree)/package/syscall/open/open_flags_TMPFILE
PK_OPEN_FLAGS_TMPFILE_PATCH       := $(PK_OPEN_FLAGS_TMPFILE_DIR)/patch
PK_OPEN_FLAGS_TMPFILE_BSBIT       := open_flags_TMPFILE-$(PK_OPEN_FLAGS_TMPFILE_VERSION).bsbit
PK_OPEN_FLAGS_TMPFILE_OUT         :=
PK_OPEN_FLAGS_TMPFILE_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_OPEN_FLAGS_TMPFILE_TAR         := $(CONFIG_PK_OPEN_FLAGS_TMPFILE_TAR)
PK_OPEN_FLAGS_TMPFILE_PACKAGE     :=
PK_OPEN_FLAGS_TMPFILE_CONFIG      := $(CONFIG_PK_OPEN_FLAGS_TMPFILE_CONFIG)
SUB_TARGET                  += $(PK_OPEN_FLAGS_TMPFILE_DIR)/build

PK_OPEN_FLAGS_TMPFILE_sh = $(CONFIG_SHELL) $(PK_OPEN_FLAGS_TMPFILE_SCRIPTS) \
                 $1X \
                 $(PK_OPEN_FLAGS_TMPFILE_NAME)X \
                 $(PK_OPEN_FLAGS_TMPFILE_VERSION)X \
                 $(PK_OPEN_FLAGS_TMPFILE_SITE)X \
                 $(PK_OPEN_FLAGS_TMPFILE_GITHUB)X \
                 $(PK_OPEN_FLAGS_TMPFILE_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_OPEN_FLAGS_TMPFILE_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_OPEN_FLAGS_TMPFILE_SUBNAME)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_CONFIG)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_LIBPATH)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_CPPFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_DPKCONFIG)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_CFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_LDFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_CXXFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_ASFLAGS)X \
                 $(PK_OPEN_FLAGS_TMPFILE_BSBIT)X \
                 $(PK_OPEN_FLAGS_TMPFILE_BUILDARCH)X \
		 $(PK_OPEN_FLAGS_TMPFILE_CSRCFILE)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_SRCFILE)X \
                 $(CONFIG_PK_OPEN_FLAGS_TMPFILE_CSRCFILE)

$(PK_OPEN_FLAGS_TMPFILE_DIR)/build: FORCE
	$(Q)$(call PK_OPEN_FLAGS_TMPFILE_sh,$(srctree))
