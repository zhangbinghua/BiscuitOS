################################################
#
# open_flags_RDONLY
#
################################################
PK_OPEN_FLAGS_RDONLY_NAME        := open_flags_RDONLY
PK_OPEN_FLAGS_RDONLY_VERSION     := $(CONFIG_PK_OPEN_FLAGS_RDONLY_VERSION)
PK_OPEN_FLAGS_RDONLY_SITE        := $(CONFIG_PK_OPEN_FLAGS_RDONLY_URL)
PK_OPEN_FLAGS_RDONLY_GITHUB      := $(CONFIG_PK_OPEN_FLAGS_RDONLY_GITHUB)
PK_OPEN_FLAGS_RDONLY_SUBNAME     :=
PK_OPEN_FLAGS_RDONLY_BUILDARCH   := $(CONFIG_PK_OPEN_FLAGS_RDONLY_BUILDARCH)
PK_OPEN_FLAGS_RDONLY_DIR         := $(srctree)/package/syscall/open/open_flags_RDONLY
PK_OPEN_FLAGS_RDONLY_PATCH       := $(PK_OPEN_FLAGS_RDONLY_DIR)/patch
PK_OPEN_FLAGS_RDONLY_BSBIT       := open_flags_RDONLY-$(PK_OPEN_FLAGS_RDONLY_VERSION).bsbit
PK_OPEN_FLAGS_RDONLY_OUT         :=
PK_OPEN_FLAGS_RDONLY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_OPEN_FLAGS_RDONLY_TAR         := $(CONFIG_PK_OPEN_FLAGS_RDONLY_TAR)
PK_OPEN_FLAGS_RDONLY_PACKAGE     :=
PK_OPEN_FLAGS_RDONLY_CONFIG      := $(CONFIG_PK_OPEN_FLAGS_RDONLY_CONFIG)
SUB_TARGET                  += $(PK_OPEN_FLAGS_RDONLY_DIR)/build

PK_OPEN_FLAGS_RDONLY_sh = $(CONFIG_SHELL) $(PK_OPEN_FLAGS_RDONLY_SCRIPTS) \
                 $1X \
                 $(PK_OPEN_FLAGS_RDONLY_NAME)X \
                 $(PK_OPEN_FLAGS_RDONLY_VERSION)X \
                 $(PK_OPEN_FLAGS_RDONLY_SITE)X \
                 $(PK_OPEN_FLAGS_RDONLY_GITHUB)X \
                 $(PK_OPEN_FLAGS_RDONLY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_OPEN_FLAGS_RDONLY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_OPEN_FLAGS_RDONLY_SUBNAME)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_CONFIG)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_LIBPATH)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_CPPFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_DPKCONFIG)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_CFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_LDFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_CXXFLAGS)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_ASFLAGS)X \
                 $(PK_OPEN_FLAGS_RDONLY_BSBIT)X \
                 $(PK_OPEN_FLAGS_RDONLY_BUILDARCH)X \
		 $(PK_OPEN_FLAGS_RDONLY_CSRCFILE)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_SRCFILE)X \
                 $(CONFIG_PK_OPEN_FLAGS_RDONLY_CSRCFILE)

$(PK_OPEN_FLAGS_RDONLY_DIR)/build: FORCE
	$(Q)$(call PK_OPEN_FLAGS_RDONLY_sh,$(srctree))
