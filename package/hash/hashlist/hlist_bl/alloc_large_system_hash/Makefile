################################################
#
# alloc_large_system_hash
#
################################################
PK_HLIST_BL_ALLOC_SYS_HASH_NAME        := alloc_large_system_hash
PK_HLIST_BL_ALLOC_SYS_HASH_VERSION     := $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_VERSION)
PK_HLIST_BL_ALLOC_SYS_HASH_SITE        := $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_URL)
PK_HLIST_BL_ALLOC_SYS_HASH_GITHUB      := $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_GITHUB)
PK_HLIST_BL_ALLOC_SYS_HASH_SUBNAME     :=
PK_HLIST_BL_ALLOC_SYS_HASH_BUILDARCH   := $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_BUILDARCH)
PK_HLIST_BL_ALLOC_SYS_HASH_DIR         := $(srctree)/package/hash/hashlist/hlist_bl/alloc_large_system_hash
PK_HLIST_BL_ALLOC_SYS_HASH_PATCH       := $(PK_HLIST_BL_ALLOC_SYS_HASH_DIR)/patch
PK_HLIST_BL_ALLOC_SYS_HASH_BSBIT       := alloc_large_system_hash-$(PK_HLIST_BL_ALLOC_SYS_HASH_VERSION).bsbit
PK_HLIST_BL_ALLOC_SYS_HASH_OUT         :=
PK_HLIST_BL_ALLOC_SYS_HASH_SCRIPTS     := $(srctree)/scripts/package/package-module.sh
PK_HLIST_BL_ALLOC_SYS_HASH_TAR         := $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_TAR)
PK_HLIST_BL_ALLOC_SYS_HASH_PACKAGE     :=
PK_HLIST_BL_ALLOC_SYS_HASH_CONFIG      := $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_CONFIG)
SUB_TARGET                  += $(PK_HLIST_BL_ALLOC_SYS_HASH_DIR)/build

PK_HLIST_BL_ALLOC_SYS_HASH_sh = $(CONFIG_SHELL) $(PK_HLIST_BL_ALLOC_SYS_HASH_SCRIPTS) \
                 $1X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_NAME)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_VERSION)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_SITE)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_GITHUB)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_SUBNAME)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_CONFIG)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_LIBPATH)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_CPPFLAGS)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_DPKCONFIG)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_CFLAGS)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_LDFLAGS)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_CXXFLAGS)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_ASFLAGS)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_BSBIT)X \
                 $(PK_HLIST_BL_ALLOC_SYS_HASH_BUILDARCH)X \
		 $(PK_HLIST_BL_ALLOC_SYS_HASH_CSRCFILE)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_SRCFILE)X \
                 $(CONFIG_PK_HLIST_BL_ALLOC_SYS_HASH_CSRCFILE)

$(PK_HLIST_BL_ALLOC_SYS_HASH_DIR)/build: FORCE
	$(Q)$(call PK_HLIST_BL_ALLOC_SYS_HASH_sh,$(srctree))
